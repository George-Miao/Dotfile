// @collapse
{
  /* -------------------------------------------------------------------------- */
  /*                               General Config                               */
  /* -------------------------------------------------------------------------- */
  "editor.fontFamily": "Cascadia Code, JetBrainsMono NerdFont, Jetbrains Mono",
  // "editor.fontFamily": "Operator Mono SSm Lig Book, OperatorMonoSSmLig-Book, Cascadia Code, JetBrainsMono NerdFont, Jetbrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  // "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.linkedEditing": true,
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "Cascadia Code",
  "terminal.integrated.cursorStyle": "line",
  "workbench.colorTheme": "Bearded Theme Light",
  "workbench.iconTheme": "material-icon-theme",
  "explorer.decorations.badges": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "http.proxy": "http://127.0.0.1:7890",
  /* -------------------------------------------------------------------------- */
  /*                                 Tool Config                                */
  /* -------------------------------------------------------------------------- */
  /* VS intellicode */
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  /* Git */
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.allowForcePush": true,
  /* Prettier */
  "prettier.quoteProps": "consistent",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "avoid",
  "prettier.useEditorConfig": true,
  "prettier.trailingComma": "none",
  "prettier.vueIndentScriptAndStyle": false,
  /* Spell Checker */
  "cSpell.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.experimental.enableRegexpView": true,
  "cSpell.customDictionaries": {
    "global": {
      "addWords": true,
      "description": "Global dictionary",
      "path": "/home/pop/.config/Code - Insiders/User/dictionary.txt",
      "name": "Global",
    }
  },
  /* Todo Tree */
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "FUCK",
    "TODO",
    "XXX",
    "[ ]",
    "@todo",
    "todo"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  /* Gitlens */
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultGravatarsStyle": "mp",
  "gitlens.statusBar.enabled": false,
  /* importSorter */
  "importSorter.importStringConfiguration.hasSemicolon": false,
  /* AutoCloseTag */
  "auto-close-tag.activationOnLanguage": [
    "*"
  ],
  "auto-close-tag.disableOnLanguage": [
    "typescript",
    "rust"
  ],
  /* Minify */
  "minify.minifyExistingOnSave": true,
  /* Qiniu */
  "qiniu.enable": true,
  /* Vetur */
  "vetur.format.enable": true,
  "vetur.format.defaultFormatter.html": "prettyhtml",
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.scriptInitialIndent": true,
  "vetur.format.styleInitialIndent": true,
  /* Project Manager */
  "projectManager.git.baseFolders": [
    "/home/pop/Dropbox/Dev/Projects",
    "/Users/Miao/Dropbox/Dev"
  ],
  "projectManager.sortList": "Recent",
  "projectManager.groupList": true,
  "projectManager.removeCurrentProjectFromList": false,
  "projectManager.showProjectNameInStatusBar": true,
  /* Code Snap */
  "codesnap.realLineNumbers": true,
  "codesnap.backgroundColor": "#ccc",
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 20px 68px",
  /* Color info */
  "colorInfo.languages": [
    {
      "selector": "css",
      "colors": "css"
    },
    {
      "selector": "sass",
      "colors": "css"
    },
    {
      "selector": "scss",
      "colors": "css"
    },
    {
      "selector": "less",
      "colors": "css"
    },
    {
      "selector": "jsonc",
      "colors": "css"
    }
  ],
  /* Mongo DB */
  "mdb.confirmRunAll": false,
  "mdb.defaultLimit": 10000,
  /* DrawIO */
  "hediet.vscode-drawio.local-storage": "",
  /* Scroll Faster */
  "scrollFaster.scrollByLines": 4,
  "scrollFaster.cursorFollowsScroll": true,
  /********** Language Config **********/
  /** Typescript **/
  "typescript.format.enable": true,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  /** Javascript **/
  "javascript.format.enable": true,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  /* Json */
  "json.format.enable": true,
  /* Python */
  "python.insidersChannel": "weekly",
  "python.languageServer": "Pylance",
  /* Rust */
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.lruCapacity": 1024,
  "rust-analyzer.assist.importGranularity": "module",
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.inlayHints.smallerHints": true,
  "rust-analyzer.assist.importPrefix": "crate",
  /* Golang */
  "go.useLanguageServer": true,
  /* SVG */
  "svg.preview.mode": "svg",
  "togglequotes.chars": [
    "\"",
    "'",
    "`"
  ],
  /* Workspace Sidebar */
  "workspaceSidebar.folder": "~/Dropbox/Dev/Workspaces",
  /* -------------------------------------------------------------------------- */
  /*                          Language Overwrite Config                         */
  /* -------------------------------------------------------------------------- */
  "files.associations": {
    "MoeDarkrc": "properties",
    "*.kvconfig": "properties",
    "*.html": "html",
    "*.json": "jsonc",
    "config": "i3"
  },
  "[typescript]": {},
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust",
    "editor.tabSize": 4
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {},
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[swig]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[Log]": {
    "editor.wordWrap": "off",
    "editor.fontSize": 11
  },
  "[code-runner-output]": {
    "editor.wordWrap": "off",
    "editor.fontSize": 11,
    "editor.lineHeight": 1.1
  },
  "security.workspace.trust.untrustedFiles": "open",
  "redhat.telemetry.enabled": false,
  "[dockercompose]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
}
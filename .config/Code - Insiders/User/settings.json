{
  /* -------------------------------------------------------------------------- */
  /*                               General Config                               */
  /* -------------------------------------------------------------------------- */
  /* Editor */
  // "editor.fontFamily": "Operator Mono SSm Lig Book, OperatorMonoSSmLig-Book, Cascadia Code, JetBrainsMono NerdFont, Jetbrains Mono",
  "editor.fontFamily": "Cascadia Code, JetBrainsMono NerdFont, Jetbrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.suggestSelection": "first",
  "editor.bracketPairColorization.enabled": true,
  // "editor.formatOnSave": false,
  "editor.formatOnSave": true,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.suggest.insertMode": "replace",
  // "editor.snippetSuggestions": "",
  "editor.linkedEditing": true,
  "editor.inlineSuggest.enabled": true,
  "editor.inlayHints.enabled": "on",
  "editor.renderWhitespace": "all",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  /* Security */
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "window.titleBarStyle": "custom",
  "window.autoDetectColorScheme": true,
  /* Terminal */
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.fontFamily": "Cascadia Code",
  "terminal.integrated.cursorStyle": "line",
  /* Workbench */
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Bearded Theme Solarized Light",
  "workbench.preferredDarkColorTheme": "Everforest Dark",
  "workbench.preferredLightColorTheme": "Bearded Theme Solarized Light",
  "workbench.colorCustomizations": {
    // Name of the theme you are currently using
    "[Bearded Theme Light][Bearded Theme Solarized Light]": {
      "editorInlayHint.foreground": "#787878bf",
      "editorInlayHint.background": "#d1d1d150",
      "editorInlayHint.parameterForeground": "#787878bf",
      "editorInlayHint.parameterBackground": "#d1d1d150",
    },
  },
  "workbench.editorAssociations": {
    "*.pdf": "latex-workshop-pdf-hook",
    "git-rebase-todo": "gitlens.rebase"
  },
  /* Explorer */
  "explorer.decorations.badges": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  /* DiffEditor */
  "diffEditor.ignoreTrimWhitespace": false,
  /* Files */
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  // "http.proxy": "http://proxy.lan:7890",
  /* -------------------------------------------------------------------------- */
  /*                                 Tool Config                                */
  /* -------------------------------------------------------------------------- */
  /* SqlTools */
  "sqltools.format": {
    "reservedWordCase": "upper",
    "linesBetweenQueries": 2,
    "language": "sql"
  },
  "sqltools.useNodeRuntime": true,
  /* VS intellicode */
  // "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  /* Git */
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.ignoreRebaseWarning": true,
  "git.allowForcePush": true,
  "git.confirmSync": false,
  /* Copilot */
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  /* Prettier */
  "prettier.quoteProps": "consistent",
  "prettier.singleQuote": true,
  "prettier.semi": false,
  "prettier.bracketSpacing": true,
  "prettier.arrowParens": "avoid",
  "prettier.useEditorConfig": true,
  "prettier.trailingComma": "none",
  "prettier.vueIndentScriptAndStyle": false,
  /* Spell Checker */
  "cSpell.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.experimental.enableRegexpView": true,
  "cSpell.customDictionaries": {
    "global": {
      "addWords": true,
      "description": "Global dictionary",
      "path": "~/.config/Code - Insiders/User/dictionary.txt",
      "name": "Global"
    }
  },
  /* Todo Tree */
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules"
  ],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "FUCK",
    "TODO",
    "XXX",
    "[ ]",
    "@todo",
    "todo",
    "WIP"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  /* Gitlens */
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.defaultGravatarsStyle": "mp",
  /* importSorter */
  "importSorter.importStringConfiguration.hasSemicolon": false,
  /* Minify */
  "minify.minifyExistingOnSave": true,
  /* Project Manager */
  "projectManager.git.baseFolders": [
    "/home/pop/Sync/Dev/Projects",
    "/Users/pop/Sync/Dev/Projects"
  ],
  "projectManager.groupList": true,
  "projectManager.removeCurrentProjectFromList": false,
  "projectManager.showProjectNameInStatusBar": true,
  /* Mongo DB */
  "mdb.confirmRunAll": false,
  "mdb.defaultLimit": 10000,
  /* Redhat */
  "redhat.telemetry.enabled": false,
  /* Trailing Space */
  "trailing-spaces.trimOnSave": true,
  /* LaTeX Workshop */
  "latex-workshop.intellisense.package.enabled": true,
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-pdf",
        "-xelatex",
        "-outdir=%OUTDIR%",
        "%DOC%"
      ],
      "env": {}
    }
  ],
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.ist",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.nav",
    "*.dvi",
    "*.synctex.gz"
  ],
  "latex-workshop.latex.outDir": "out",
  "latex-workshop.latex.autoBuild.interval": 1000,
  "latex-workshop.view.pdf.viewer": "tab",
  /* Toggle Quotes */
  "togglequotes.chars": [
    "\"",
    "'",
    "`"
  ],
  /* Workspace Sidebar */
  "workspaceSidebar.folder": "~/Dev/Workspaces",
  /* Even Better Toml */
  "evenBetterToml.formatter.alignEntries": true,
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.arrayAutoExpand": false,
  "evenBetterToml.formatter.trailingNewline": true,
  /* Vale */
  "vale.core.useCLI": true,
  /* Bracket Lens */
  "bracketLens.mode": "on-save",
  /* Svelte */
  "svelte.enable-ts-plugin": true,
  /* html-css-completion */
  "html-css-class-completion.enableEmmetSupport": true,
  "html-css-class-completion.HTMLLanguages": [
    "html",
    "vue",
    "razor",
    "blade",
    "handlebars",
    "twig",
    "django-html",
    "php",
    "markdown",
    "erb",
    "ejs",
    "svelte"
  ],
  /* Windicss Intellisense */
  "windicss.sortOnSave": true,
  /* Grammarly */
  "grammarly.files.include": [
    "**/README.md",
    "**/readme.md",
    "**/*.txt",
    "posts/**/*.md"
  ],
  "typst-lsp.exportPdf": "onType",
  /* -------------------------------------------------------------------------- */
  /*                               Language Config                              */
  /* -------------------------------------------------------------------------- */
  /** Typescript **/
  "typescript.format.enable": true,
  "typescript.format.semicolons": "remove",
  "typescript.preferences.quoteStyle": "single",
  "typescript.updateImportsOnFileMove.enabled": "always",
  /** Javascript **/
  "javascript.format.enable": true,
  "javascript.format.semicolons": "remove",
  "javascript.preferences.quoteStyle": "single",
  "javascript.updateImportsOnFileMove.enabled": "always",
  /* Json */
  "json.format.enable": true,
  /* Python */
  "python.languageServer": "Pylance",
  "python.formatting.provider": "yapf",
  /* Rust */
  "rust-analyzer.imports.granularity.enforce": true,
  "rust-analyzer.imports.granularity.group": "crate",
  "rust-analyzer.imports.prefix": "crate",
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.lru.capacity": 1024,
  "rust-analyzer.cargo.features": "all",
  "rust-analyzer.check.command": "clippy",
  /* Golang */
  "go.useLanguageServer": true,
  /* SVG */
  "svg.preview.mode": "img",
  /* Haskell */
  "haskell.ghcupExecutablePath": "/home/pop/.ghcup/bin/ghcup",
  "haskell.manageHLS": "GHCup",
  /* Lua */
  "Lua.telemetry.enable": false,
  "files.associations": {
    "MoeDarkrc": "properties",
    "*.kvconfig": "properties",
    "*.html": "html",
    "*.json": "jsonc",
    "config": "i3",
    "*.swig": "html",
    "*.rush": "rust"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[swig]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[Log]": {
    "editor.fontSize": 11
  },
  "[code-runner-output]": {
    "editor.fontSize": 11,
    "editor.lineHeight": 1.1
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "[javascript][typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop",
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[markdown][latex][plaintext][css][postcss]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 100
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma-insider"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "svelte.plugin.svelte.note-new-transformation": false,
  "github-actions.org-features": true,
  "[caddyfile]": {
    "editor.defaultFormatter": "matthewpi.caddyfile-support"
  },
  "C_Cpp.intelliSenseEngine": "disabled",
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "remote.SSH.externalSSH_ASKPASS": true,
  "leetcode.workspaceFolder": "/Users/pop/.leetcode",
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "jupyter.askForKernelRestart": false,
  "git.confirmForcePush": false,
  "hexeditor.columnWidth": 28,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  "xml.symbols.maxItemsComputed": 1000,
  "xml.server.vmargs": "-Xmx128M",
}